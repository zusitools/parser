    <xs:complexType>
      <xs:sequence>
        <xs:element name="eta" type="Kennfeld"/>
        <xs:element ref="GetrStufen"/>
      </xs:sequence>
      <xs:attribute name="FuellungsGeregelt" type="boolean"/>
